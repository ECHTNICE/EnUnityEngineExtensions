{
  "metadata": [
    {
      "src": [
        {
         "files": ["**/*.csproj" ],
         "exclude": [ "**/bin/**", "**/obj/**"],
         "src": "../"
        }
      ],
	  "filter": "filterConfig.yml",
	  "globalNamespaceId": "Global",
      "dest": "api",
	  "msbuildProperties":[{"ENABLE_EN_2D_EXTENSIONS":"true"}],
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**",
		  "docs"
        ]
      }
    ],
	"globalMetadata": {
      "_appTitle": "EnUnityEngineExtensions Documentation",
	  "_appFooter": "Copyright Â© ECHTNICE",
	  "_appLogoPath": "Images/echtnice_50x50.png"
	},
    "dest": "../docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
	"template": [ "statictoc" ],
	"template comment out": [ "default" ],	 
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}